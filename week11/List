a = [1, 2, 3]
print(a)
print(a[0])
print(a[0] + a[2])
print(a[-1],'\n')
a = [1, 2, 3, ['a', 'b', 'c']]
print(a)
print(a[-1][0])
print( a[-1][1])
print( a[-1][2],'\n')
a = [1, 2, 3,4,5,6,7,8,9]
print(a[2:])
print(a[:4])
print(a[:])
print(a[2:5])
print(a[-1:])
print(a[-2:],'\n')

#리스트 더하기

b = [10,11,12,13]
print(a+b)

#리스트 반복하기

print(a*3)
